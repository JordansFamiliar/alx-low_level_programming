#include "_putchar.c"

int _putchar(char);
void print_alphabet(void);
void print_alphabet_x10(void);

/**
* print_alphabet - prints alphabet in lowercase to stdout
*
* Return: void
*/
void print_alphabet(void)
{
	int i = 97;

	while (i < 123)
	{
		_putchar(i);
	}
	_putchar(10);
}

/**
* see print_alphabet - x10
*
* Return: void
*/
void print_alphabet_x10(void)
{
	int a = 97;
	int i = 1;

	while (i < 11)
	{
		while (a < 123)
		{
			_putchar(a);
		}
		_putchar(10);
	}
}
